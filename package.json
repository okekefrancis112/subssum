{
  "name": "subssum-v2",
  "version": "1.1.0",
  "description": "A Property Technology Codebase",
  "main": "server.js",
  "scripts": {
    "tsc": "tsc",
    "bitbucket:build": "tsc",
    "build": "tsc && npm run create-view && npm run copy-files && npm run copy-greeting",
    "dev:build": "tsc && sudo npm run create-view && sudo npm run copy-files && sudo npm run copy-greeting",
    "dev:user": "ts-node-dev source/servers/user.server.ts",
    "start": "node ./build/servers/user.server.js",
    "runBuild": "pm2 start ecosystem.yml --only node-app",
    "dev:admin": "ts-node-dev source/servers/admin.server.ts",
    "dev:consumer": "ts-node-dev source/servers/consumer.server.ts",
    "dev:all": "start npm run dev:user && start npm run dev:admin",
    "create-view": "mkdir -p ./build/views",
    "copy-files": "cp -r ./source/views/* ./build/views/",
    "copy-greeting": "cp ./source/index.html ./build/",
    "clean": "rm -rf ./build",
    "test": "SET NODE_ENV=test && jest --testTimeout=10000 --forceExit --coverage --verbose --runInBand"
  },
  "keywords": [
    "subssum",
    "proptech"
  ],
  "author": "Valentine Offiah",
  "license": "ISC",
  "dependencies": {
    "@types/node-cron": "^3.0.7",
    "aws-sdk": "^2.1272.0",
    "axios": "^1.2.1",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "bullmq": "^1.51.3",
    "cors": "^2.8.5",
    "cron": "^2.3.0",
    "degenerator": "^5.0.1",
    "discord-webhook-node": "^1.1.8",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "express-handlebars": "^7.0.7",
    "fingerprintjs2": "^2.1.4",
    "getmac": "^5.20.0",
    "google-auth-library": "^9.14.0",
    "handlebars": "^4.7.7",
    "ioredis": "^5.3.0",
    "joi": "^17.7.0",
    "joi-password-complexity": "^5.1.0",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^8.5.1",
    "keystone-email": "^1.1.0",
    "mailgun-js": "^0.22.0",
    "moment": "^2.29.4",
    "mongoose": "^6.7.5",
    "node-cron": "^3.0.2",
    "pdf-creator-node": "^2.3.5",
    "pug": "^3.0.2",
    "redis": "^4.6.4",
    "sinon": "^15.0.4",
    "string-similarity-js": "^2.1.4",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/bull": "^4.10.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/express-fileupload": "^1.4.1",
    "@types/fingerprintjs2": "^2.0.0",
    "@types/jest": "^29.4.0",
    "@types/joi": "^17.2.3",
    "@types/json2csv": "^5.0.3",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/mailgun-js": "^0.22.13",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^18.13.0",
    "@types/sinon": "^10.0.14",
    "@types/supertest": "^2.0.12",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.4.3",
    "nodemon": "^2.0.20",
    "prettier": "3.0.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.3"
  }
}
